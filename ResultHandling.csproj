<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>ResultWrapper</PackageId>
		<Version>1.0.0</Version>
		<Authors>Yogesh Karpe</Authors>
		<Company>YogeshKarpe</Company>
		<Description>Lightweight Result&lt;T&gt; pattern with HTTP status code, and ActionResult integration for clean ASP.NET Core APIs.</Description>
		<PackageTags>Result Pattern ASP.NETCore ErrorHandling</PackageTags>
		<RepositoryUrl>https://github.com/YogeshKarpe/ResultHandling</RepositoryUrl>
		<Title>Result Handling</Title>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Abstractions" Version="2.3.0" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.3.0" />
	</ItemGroup>

</Project>
